--Open a link or New Tab (forground)
acConsumePhysicalInput(1) 
if acGetMouseCursorType() ~= "HAND" then acSendKeys("^t")
else 
   acMouseMove(gsx, gsy) 
   acSendControlDown()
   acDelay(25)
   acSendShiftDown() 
   acMouseClick(gsx, gsy, 2, 1, 1) 
   acDelay(25)
   acSendShiftUp()   
   acSendControlUp()
end
acConsumePhysicalInput(0)
